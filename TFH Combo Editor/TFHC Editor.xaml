<Window x:Class="TFH_Combo_Editor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TFH_Combo_Editor"
        mc:Ignorable="d"
        Title="TFHC Editor" Height="480" Width="640" Background="White">
    <Grid>
        <TextBlock x:Name="Inputs" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Inputs" VerticalAlignment="Top"/>
        <ScrollViewer HorizontalAlignment="Left" Height="312" Margin="47,55,0,0" VerticalAlignment="Top" Width="469">
            <ListBox x:Name="inputListBox" HorizontalAlignment="Left" Height="1394" VerticalAlignment="Top" Width="459"/>
        </ScrollViewer>
        <Button x:Name="SaveButton" Content="Save" Click="SaveButton_Click" HorizontalAlignment="Left" Margin="533,111,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Window>
